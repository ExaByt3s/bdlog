<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <bdlogroot>$(SolutionDir)..</bdlogroot>
    <bdlogoutput>$(bdlogroot)\output</bdlogoutput>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>11.0.50214.1</_ProjectFileVersion>
    <_PropertySheetDisplayName>bdlog common</_PropertySheetDisplayName>
    <OutDir>$(bdlogroot)\output\$(PlatformName)\$(Configuration)\</OutDir>
    <IntDir>$(SolutionDir)intermediate\$(ProjectName)\$(Configuration)\</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalOptions>/Wall %(AdditionalOptions)</AdditionalOptions>
      <AdditionalIncludeDirectories>$(bdlogroot)\include;$(bdlogroot)\thirdsrc\include;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>WIN32;_WINDOWS;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <PrecompiledHeader>Use</PrecompiledHeader>
    </ClCompile>
    <Link>
      <GenerateDebugInformation>true</GenerateDebugInformation>
      <TargetMachine>MachineX86</TargetMachine>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="bdlogroot">
      <Value>$(bdlogroot)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="bdlogoutput">
      <Value>$(bdlogoutput)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>